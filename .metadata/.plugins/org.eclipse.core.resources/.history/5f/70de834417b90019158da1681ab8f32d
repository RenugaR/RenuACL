package org.javaprograms;

public class NumbersString {

	public static void main(String[] args) {
		String s="(10-6)=46";
		int length = s.length();
		int temp=length;
		for (int i = 0; s.charAt(i)!='='; i++) {
			 temp =temp-1;
		}
		for (int i = length-temp+1; i<length  ; i++) {
			System.out.print(s.charAt(i));
		}
	}

}
