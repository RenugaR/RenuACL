package JavaPrograms;

public class TwoTimesRepeat {
	
	public static int Factorial(int num) {
		int fact=1;
		while(num>0)
		for (int i = 1; i <=num; i++) {
			fact=fact*i;
		}System.out.println(fact);
		return fact;
	}
	public static void main(String[] args) {
		System.out.println(Factorial(5));
	}
}
		 