package org.javaprograms;

public class NumbersString {

	public static void main(String[] args) {
		String s="(10-6)=4";
		int length = s.length();
		int temp=length;
		for (int i = 0; s.charAt(i)!='='; i++) {
			 temp =temp-1;
		}System.out.println(temp);
		for (int i = length-temp; i<length  ; i++) {
			System.out.print(s.charAt(i));
		}
	}

}
