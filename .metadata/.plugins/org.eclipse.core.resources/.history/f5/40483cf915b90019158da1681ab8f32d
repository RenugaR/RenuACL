package org.javaprograms;

public class NumbersString {

	public static void main(String[] args) {
		String s="(10-6)=4";
		int temp=0;
		int length = s.length();
		for (int i = 0; s.charAt(i)=='='; i++) {
			 temp =length-1;
		}System.out.println(temp);
		for (int i = length-temp; length<i  ; i++) {
			System.out.print(s.charAt(i));
		}
	}

}
